<?xml version="1.0" encoding="UTF-8"?>

<configuration>
  <jdbc driver="com.mysql.jdbc.Driver"
        url="jdbc:mysql://192.168.0.16:3306/boss2?generateSimpleParameterMetadata=true"
        user="dev01" password="dev01@dev01">
  </jdbc>

  <baseDir>./target</baseDir>

  <base>
    <mapperExt>com.dhf.boss.security.dao.security.ext</mapperExt>
    <mapperXmlExt>xml/com.dhf.boss.security.dao.security.ext</mapperXmlExt>
    <controller>com.dhf.boss.security.controller</controller>
    <service>com.dhf.boss.security.service</service>
    <param>com.dhf.boss.security.param</param>
    <jsp>jsp</jsp>
    <mybatis>
      <mapper>com.dhf.boss.security.dao.security.gen</mapper>
      <mapperXml>xml/com.dhf.boss.security.dao.security.gen</mapperXml>
      <mapperModel>com.dhf.boss.security.dao.security.gen.entity</mapperModel>
    </mybatis>
  </base>

  <!--
    mapperExt: 是否生成ext
    service：是否生成业务类
    controller：是否生成控制器
    param: 是否生成参数实体
    jsp：是否生成jsp页面
    page: 是否生成分页方法
    list: 是否生成list方法
    controller="true" service="true" param="true" jsp="true" page="true" list="true"
   -->
  <tables mapperExt="false" controller="false" service="false" param="false" jsp="false" page="false" list="false">
    <table name="t_function"/>
    <table name="t_role_city"/>
    <table name="t_operator_log"/>
    <table name="t_op_role_rel"/>
    <table name="t_op_function"/>
    <table name="t_role_menu"/>
    <table name="t_role_function"/>
    <table name="t_menu"/>
    <table name="t_op_city"/>
    <table name="t_op_menu"/>
    <table name="t_operator"/>
    <table name="t_role"/>
  </tables>
</configuration>