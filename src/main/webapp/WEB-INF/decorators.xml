<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/views">
  <excludes>
    <pattern>/operator/login.html*</pattern>
    <pattern>/operator/logout.html*</pattern>
  </excludes>

  <decorator name="layout" page="layout/layout.jsp">
    <pattern>/*</pattern>
  </decorator>
  <decorator name="dialog" page="layout/dialog.jsp">
    <pattern>/*/*Dialog.html</pattern>
  </decorator>

  <decorator name="tiles" page="layout/tiles.jsp">
    <pattern>/*/*Tiles.html</pattern>
  </decorator>
</decorators>